<?xml version="1.0" encoding="UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zk/config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.zkoss.org/2005/zk/config http://www.zkoss.org/2005/zk/config/zk.xsd">

    <library-property>
        <name>org.zkoss.theme.preferred</name>
        <value>iceblue_c</value>
    </library-property>
    <!-- set true if you want to debug JavaScript.
        Once it's true, the uncompressed JavaScript files will be loaded.
    -->
    <client-config>
        <debug-js>true</debug-js>
    </client-config>
    <library-property>
        <name>org.zkoss.web.servlet.http.embedded.enabled</name>
        <value>true</value>
    </library-property>
    <listener>
        <listener-class>io.keikai.devref.util.ParameterImportComposer</listener-class>
    </listener>
<!--    <library-property>-->
<!--        <name>io.keikai.import.cache</name>-->
<!--        <value>true</value>-->
<!--    </library-property>-->
    <!-- keikai configurable properties. See https://doc.keikai.io/dev-ref/Configuration
    <library-property>
        <name>io.keikai.export.cache</name>
        <value>true</value>
    </library-property>
    <library-property>
        <name>io.keikai.ui.keepCellSelection</name>
        <value>false</value>
    </library-property>
    <library-property>
        <name>io.keikai.ui.Spreadsheet.customFontFamily</name>
        <value>Roboto, Helvetica Neue</value>
    </library-property>
    -->
</zk>
