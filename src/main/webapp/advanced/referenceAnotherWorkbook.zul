<zk xmlns="http://www.zkoss.org/2005/zul"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.zkoss.org/2005/zul http://keikai.io/2019/zul/zul.xsd">
	<window hflex="1" vflex="1" apply="io.keikai.devref.advanced.BookSeriesComposer">
		<groupbox width="100%" height="200px"><caption label="profile.xlsx"></caption>
			<spreadsheet id="profile" hflex="1"  vflex="1" showFormulabar="true" showContextMenu="true"
				showToolbar="false" showSheetbar="false" maxVisibleRows="2" maxVisibleColumns="7"/>
		</groupbox>
		<groupbox width="100%" vflex="1"><caption label="resume.xlsx"></caption>
			<spreadsheet id="resume" hflex="1" vflex="1" showFormulabar="true" showContextMenu="true"
				showToolbar="true" showSheetbar="true" showSheetTabContextMenu="true" maxVisibleRows="12" maxVisibleColumns="6"/>
		</groupbox>
	</window>
</zk>